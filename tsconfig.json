{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"jsxImportSource": "nativewind",
		"baseUrl": ".",
		"paths": {
			"#*": ["./src/*"],
			"#components/*": ["./src/components/*"],
			"#context/*": ["./src/context/*"],
			"#utils/*": ["./src/utils/*"],
			"#app/*": ["./app/*"],
			"#assets/*": ["./assets/*"]
		},
		"noImplicitAny": false,
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}
